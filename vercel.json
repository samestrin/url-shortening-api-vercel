{
  "version": 2,
  "functions": {
    "api/shorten.js": {
      "maxDuration": 60
    },
    "api/redirect.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/shorten",
      "dest": "/api/shorten.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/redirect.js"
    }
  ]
}
